<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PanelBackgroundLight">#2d2d30</Color>
    <SolidColorBrush x:Key="PanelBackgroundLightBrush" Color="{StaticResource PanelBackgroundLight}" />

    <Color x:Key="Border">#eaeaea</Color>
    <SolidColorBrush x:Key="BorderBrush" Color="{StaticResource Border}" />
</ResourceDictionary>